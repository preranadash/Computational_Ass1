#11.D
import numpy as np

coefficients = np.array([[4, 1, 1, 0, 1],
                         [-1, -3, 1, 1, 0],
                         [2, 1, 5, -1, -1],
                        [-1, -1, -1, 4, 0],
                        [0, 2, -1, 1, 4]])

constants = np.array([6, 6, 6, 6, 6])

solution = np.linalg.solve(coefficients, constants)
print("Solution:", solution)
